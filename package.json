{
  "name": "trivia-app",
  "version": "1.0.0",
  "description": "A ten question trivia game",
  "main": "index.js",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "test": "xo && tsc && ava",
    "reset-test-cache": "ava --reset-cache",
    "lint": "xo",
    "dev": "webpack-dev-server --config webpack.dev.js --color",
    "build": "webpack --config webpack.prod.js",
    "build-docs": "typedoc --out docs /src",
    "storybook": "start-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahboston/trivia-app.git"
  },
  "author": "Elijah Boston",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elijahboston/trivia-app/issues"
  },
  "homepage": "https://github.com/elijahboston/trivia-app#readme",
  "dependencies": {
    "@types/html-entities": "^1.2.16",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-jsx": "^2.2.8",
    "axios": "^0.19.0",
    "core-js": "^3.3.6",
    "html-entities": "^1.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-jsx": "^3.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-regenerator": "^7.7.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.7.0",
    "@babel/register": "^7.6.2",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jsdom": "^12.2.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "ava": "^2.4.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-config-xo-typescript": "^0.19.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.2.1",
    "react-docgen-typescript-loader": "^3.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "xo": "^0.25.3"
  },
  "xo": {
    "prettier": true,
    "fix": true,
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "ignores": [
      "webpack.*.js"
    ]
  },
  "ava": {
    "verbose": true,
    "compileEnhancements": false,
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-node/register",
      "./test-helpers/_setup-browser.ts",
      "./test-helpers/_setup-enzyme.ts"
    ],
    "files": [
      "!dist/**/*"
    ]
  }
}
